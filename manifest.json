{
  "name": "cc-notifier",
  "description": "Should fetch the last submission and alert the user of cc verdict",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "webNavigation",
    "webRequest",
    "storage",
    "webRequestBlocking",
    "https://www.codechef.com/submit/*",
    "https://www.codechef.com/api/ide/submit"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
